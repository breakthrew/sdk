{
  "name": "@breakthrew/sdk",
  "version": "0.0.2",
  "main": "./lib/index.js",
  "description": "BreakThrew Software Development Kit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/breakthrew/sdk.git"
  },
  "files": [
    "dist",
    "lib"
  ],
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "js-cookie": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "aws-sdk": "^2.849.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "fs-extra": "^9.1.0",
    "husky": "^3.1.0",
    "inquirer": "^7.3.3",
    "lint-staged": "^9.4.3",
    "mocha": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.5",
    "ora": "^5.3.0",
    "prettier": "^1.19.1",
    "rollup": "^2.39.0",
    "semver": "^7.3.4",
    "underscore": "^1.12.0"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:dist": "rollup src/index.js --file build/dist/brkthrw.min.js --format iife --name breakthrew --globals js-cookie:cookie",
    "build:umd": "rollup src/index.js --file build/lib/index.js --format umd --name breakthrew --globals js-cookie:Cookie",
    "build:dist-babel": "babel build/dist --out-dir dist",
    "build:umd-babel": "babel build/lib --out-dir lib --source-maps inline",
    "deploy": "npm-run-all deploy:*",
    "deploy:version": "node ./deploy/ver.js",
    "deploy:build": "npm run build",
    "deploy:upload": "node ./deploy/cdn.js",
    "pretest": "npm-run-all build:umd*",
    "prepublishOnly": "npm run deploy",
    "test": "mocha",
    "prompt:test": "cross-env NODE_ENV=test node ./deploy/ver.js"
  }
}
